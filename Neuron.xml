<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://github.com/Ellypse/wow-ui-source/blob/live/FrameXML/UI.xsd">


	<Texture name="NeuronCheckButtonTextureTemplate" virtual="true">
		<Size x="36" y="36"/>
		<Anchors>
			<Anchor point="CENTER">
				<Offset x="0" y="0"/>
			</Anchor>
		</Anchors>
	</Texture>




	<Button name="NeuronBarTemplate" enableMouse="true" movable="true" frameStrata="MEDIUM" virtual="true" inherits="SecureHandlerStateTemplate" >
		<Layers>
			<Layer level="BACKGROUND">
				<FontString name="$parentText" parentKey="text" inherits="GameFontNormal">
					<Anchors>
						<Anchor point="BOTTOM" relativeTo="$parent" relativePoint="TOP">
							<Offset x="0" y="1"/>
						</Anchor>
					</Anchors>
				</FontString>
				<Texture name="$parentMessageBG" parentKey="messagebg" hidden="true">
					<Size x="1" y="1"/>
					<Anchors>
						<Anchor point="TOP" relativeTo="$parent" relativePoint="BOTTOM">
							<Offset x="0" y="-1"/>
						</Anchor>
					</Anchors>
					<Color r="0" g="0" b="0" a="0.9"/>
				</Texture>
				<FontString name="$parentMessage" parentKey="message" inherits="FriendsFont_UserText" justifyV="TOP">
					<Anchors>
						<Anchor point="CENTER" relativeTo="$parentMessageBG" relativePoint="CENTER">
							<Offset x="0" y="0"/>
						</Anchor>
					</Anchors>
					<FontHeight>
						<AbsValue val="12"/>
					</FontHeight>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<CheckButton name="$parentKeyBind" parentKey="keybind" inherits="SecureActionButtonTemplate">
				<Scripts>
					<OnLoad>
						self:RegisterForClicks("AnyUp, AnyDown")
						self:RegisterEvent("ADDON_LOADED")
						self.config = {	clickAnchor = true, mouseoverAnchor = true }
					</OnLoad>
					<OnEvent>

					</OnEvent>
					<PostClick>
						--DEFAULT_CHAT_FRAME:AddMessage(self:GetName())
					</PostClick>
				</Scripts>
			</CheckButton>
		</Frames>
	</Button>




	<Button name="NeuronBindFrameTemplate" frameStrata="DIALOG" hidden="true" virtual="true">
		<Layers>
			<Layer>
				<FontString name="$parentType" parentKey="type" inherits="ChatFontSmall" nonspacewrap="true" text="bind">
					<Anchors>
						<Anchor point="TOP">
							<Offset x="0" y="-1"/>
						</Anchor>
						<Anchor point="BOTTOM">
							<Offset x="0" y="3"/>
						</Anchor>
					</Anchors>
					<Shadow>
						<Offset x="1" y="-1"/>
						<Color r="0" g="0" b="0"/>
					</Shadow>
					<Color r="1" g="1" b="1"/>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Frame name="$parentSelect" parentKey="select" setAllPoints="true" hidden="true">
				<Layers>
					<Layer level="OVERLAY">
						<Texture name="$parentReticle" file="Interface\Buttons\UI-AutoCastableOverlay">
							<TexCoords left="0.24" right="0.75" top="0.24" bottom="0.75"/>
						</Texture>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
	</Button>





	<CheckButton name="NeuronActionButtonTemplate" inherits="SecureActionButtonTemplate, SecureHandlerEnterLeaveTemplate" virtual="true">
		<Size x="43" y="43"/>
		<Layers>
			<Layer>
				<Texture name="$parentFlyoutArrow" parentKey="FlyoutArrow" hidden="true"/>
				<Texture name="$parentFlyoutBorder" parentKey="FlyoutBorder" hidden="true"/>
				<Texture name="$parentFlyoutBorderShadow" parentKey="FlyoutBorderShadow" hidden="true"/>
				<FontString name="$parentMacroName" inherits="GameFontHighlightSmallOutline">
					<Anchors>
						<Anchor point="BOTTOMLEFT">
							<Offset x="2" y="2"/>
						</Anchor>
						<Anchor point="BOTTOMRIGHT">
							<Offset x="-2" y="2"/>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="OVERLAY" textureSubLevel="1">
				<Texture name="$parentBorder" file="Interface\Buttons\UI-ActionButton-Border" hidden="true" alphaMode="ADD">
					<Size x="64" y="64"/>
					<Anchors>
						<Anchor point="CENTER">
							<Offset x="0" y="1"/>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture name="$parentGloss" file="Interface\Buttons\UI-PassiveHighlight" alphaMode="ADD">
					<Size x="63" y="63"/>
					<Anchors>
						<Anchor point="CENTER">
							<Offset x="0" y="1"/>
						</Anchor>
					</Anchors>
				</Texture>
				<Texture name="$parentFlyoutTop" file="Interface\AddOns\Neuron\Images\flyout.tga" hidden="true">
					<Size x="56" y="16"/>
					<Anchors>
						<Anchor point="TOP">
							<Offset x="0" y="5"/>
						</Anchor>
					</Anchors>
					<TexCoords left="0" right="1" top="0" bottom="0.28125"/>
				</Texture>
				<Texture name="$parentFlyoutBottom" file="Interface\AddOns\Neuron\Images\flyout.tga" hidden="true">
					<Size x="56" y="16"/>
					<Anchors>
						<Anchor point="BOTTOM">
							<Offset x="0" y="-5"/>
						</Anchor>
					</Anchors>
					<TexCoords left="0" right="1" top="0.71875" bottom="1"/>
				</Texture>
				<Texture name="$parentFlyoutLeft" file="Interface\AddOns\Neuron\Images\flyout.tga" hidden="true">
					<Size x="16" y="56"/>
					<Anchors>
						<Anchor point="LEFT">
							<Offset x="-5" y="0"/>
						</Anchor>
					</Anchors>
					<TexCoords left="0" right="0.28125" top="0" bottom="1"/>
				</Texture>
				<Texture name="$parentFlyoutRight" file="Interface\AddOns\Neuron\Images\flyout.tga" hidden="true">
					<Size x="16" y="56"/>
					<Anchors>
						<Anchor point="RIGHT">
							<Offset x="5" y="0"/>
						</Anchor>
					</Anchors>
					<TexCoords left="0.71875" right="1" top="0" bottom="1"/>
				</Texture>
				<FontString name="$parentHotKey" parentKey="hotkey" inherits="NumberFontNormal" justifyH="RIGHT">
					<Size x="43" y="10"/>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset x="-3" y="-4"/>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentCount" inherits="NumberFontNormal" justifyH="RIGHT">
					<Anchors>
						<Anchor point="BOTTOMRIGHT">
							<Offset x="-2" y="2"/>
						</Anchor>
					</Anchors>
				</FontString>

				<Texture name="$parentAutoCastable" file="Interface\Buttons\UI-AutoCastableOverlay" hidden="true">
					<Anchors>
						<Anchor point="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT"/>
					</Anchors>
					<TexCoords left="0.1875" right="0.8125" top="0.1875" bottom="0.8125"/>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Frame name="$parentIconFrame">
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset x="3" y="-3"/>
					</Anchor>
					<Anchor point="BOTTOMRIGHT">
						<Offset x="-3" y="3"/>
					</Anchor>
				</Anchors>
				<Layers>
					<Layer>
						<Texture name="$parentIcon">
							<TexCoords left="0.01" right="0.99" top="0.01" bottom="0.99"/>
						</Texture>
					</Layer>
					<Layer level="OVERLAY">
						<Texture name="$parentFlash" hidden="true">
							<Anchors>
								<Anchor point="TOPLEFT" relativeTo="$parentIcon" relativePoint="TOPLEFT"/>
								<Anchor point="BOTTOMRIGHT" relativeTo="$parentIcon" relativePoint="BOTTOMRIGHT"/>
							</Anchors>
							<Color r="1" g="0" b="0" a="0.5"/>
						</Texture>
						<Texture name="$parentRange" hidden="true">
							<Anchors>
								<Anchor point="TOPLEFT" relativeTo="$parentIcon" relativePoint="TOPLEFT"/>
								<Anchor point="BOTTOMRIGHT" relativeTo="$parentIcon" relativePoint="BOTTOMRIGHT"/>
							</Anchors>
							<Color r="1" g="1" b="1" a="0.5"/>
						</Texture>
					</Layer>
					<Layer level="BACKGROUND">
						<Texture name="$parentBackGround">
							<Size x="60" y="60"/>
							<Anchors>
								<Anchor point="CENTER">
									<Offset x="0" y="0"/>
								</Anchor>
							</Anchors>
						</Texture>
						<Texture name="$parentFiller">
							<Size x="60" y="60"/>
							<Anchors>
								<Anchor point="TOPLEFT" relativeTo="$parentIcon" relativePoint="TOPLEFT"/>
								<Anchor point="BOTTOMRIGHT" relativeTo="$parentIcon" relativePoint="BOTTOMRIGHT"/>
							</Anchors>
							<Color r="0" g="0" b="0" a="1"/>
						</Texture>
					</Layer>
				</Layers>
				<Frames>
					<Cooldown name="$parentCooldown" inherits="CooldownFrameTemplate">
						<Anchors>
							<Anchor point="TOPLEFT"/>
							<Anchor point="BOTTOMRIGHT"/>
						</Anchors>
						<Layers>
							<Layer level="OVERLAY" textureSubLevel="3">
								<FontString name="$parentTimer" parentKey="timer" inherits="GameFontNormal" hidden="true">
									<Anchors>
										<Anchor point="CENTER">
											<Offset x="0.5" y="0"/>
										</Anchor>
									</Anchors>
								</FontString>
							</Layer>
						</Layers>
						<Scripts>
							<OnLoad>
								self:SetFrameLevel(3)
								self.button = self:GetParent():GetParent()
							</OnLoad>
							<OnHide>
								self.active = nil
							</OnHide>
						</Scripts>
					</Cooldown>
					<Cooldown name="$parentAuraWatch" inherits="CooldownFrameTemplate" reverse="true">
						<Anchors>
							<Anchor point="TOPLEFT" relativeTo="$parentCooldown" relativePoint="TOPLEFT"/>
							<Anchor point="BOTTOMRIGHT" relativeTo="$parentCooldown" relativePoint="BOTTOMRIGHT"/>
						</Anchors>
						<Layers>
							<Layer level="OVERLAY" textureSubLevel="3">
								<FontString name="$parentTimer" parentKey="timer" inherits="GameFontNormal" hidden="true">
									<Anchors>
										<Anchor point="CENTER">
											<Offset x="0.5" y="0"/>
										</Anchor>
									</Anchors>
								</FontString>
							</Layer>
						</Layers>
						<Scripts>
							<OnLoad>
								self:SetFrameLevel(3)
								self.button = self:GetParent():GetParent()
								self.alphafade = true
							</OnLoad>
							<OnHide>
								self.active = nil
							</OnHide>
						</Scripts>
					</Cooldown>
				</Frames>
				<Scripts>
					<OnLoad>
						self:SetFrameLevel(2)

						local icon = _G[self:GetName().."Icon"]
						icon.filler = _G[self:GetName().."Filler"]

						if (icon and icon.filler) then

							icon.oShow = icon.Show
							icon.oHide = icon.Hide

							icon.Show = function(self) self.filler:Show() self:oShow() end
							icon.Hide = function(self) self.filler:Hide() self:oHide() end
						end
					</OnLoad>
				</Scripts>
			</Frame>
			<Frame name="$parentShine" inherits="AutoCastShineTemplate">
				<Size x="32" y="32"/>
				<Anchors>
					<Anchor point="CENTER">
						<Offset x="0" y="0"/>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="$parentHitBox" hidden="true">
				<Anchors>
					<Anchor point="TOPLEFT"/>
					<Anchor point="BOTTOMRIGHT"/>
				</Anchors>
				<Layers>
					<Layer>
						<Texture>
							<Size x="1" y="1"/>
							<Anchors>
								<Anchor point="TOPLEFT">
									<Offset x="0" y="0"/>
								</Anchor>
								<Anchor point="BOTTOMLEFT">
									<Offset x="0" y="0"/>
								</Anchor>
							</Anchors>
							<Color r="1" g="1" b="1"/>
						</Texture>
						<Texture>
							<Size x="1" y="1"/>
							<Anchors>
								<Anchor point="TOPRIGHT">
									<Offset x="0" y="0"/>
								</Anchor>
								<Anchor point="BOTTOMRIGHT">
									<Offset x="0" y="0"/>
								</Anchor>
							</Anchors>
							<Color r="1" g="1" b="1"/>
						</Texture>
						<Texture>
							<Size x="1" y="1"/>
							<Anchors>
								<Anchor point="TOPLEFT">
									<Offset x="0" y="0"/>
								</Anchor>
								<Anchor point="TOPRIGHT">
									<Offset x="0" y="0"/>
								</Anchor>
							</Anchors>
							<Color r="1" g="1" b="1"/>
						</Texture>
						<Texture>
							<Size x="1" y="1"/>
							<Anchors>
								<Anchor point="BOTTOMLEFT">
									<Offset x="0" y="0"/>
								</Anchor>
								<Anchor point="BOTTOMRIGHT">
									<Offset x="0" y="0"/>
								</Anchor>
							</Anchors>
							<Color r="1" g="1" b="1"/>
						</Texture>
					</Layer>
				</Layers>
			</Frame>
		</Frames>
		<NormalTexture name="$parentNormalTexture" file="Interface\Buttons\UI-Quickslot">
			<Anchors>
				<Anchor point="TOPLEFT" x="-12" y="12"/>
				<Anchor point="BOTTOMRIGHT" x="13" y="-13"/>
			</Anchors>
		</NormalTexture>
		<PushedTexture name="$parentPushedTexture" inherits="NeuronCheckButtonTextureTemplate" file="Interface\Buttons\UI-Quickslot-Depress">
			<Anchors>
				<Anchor point="TOPLEFT" x="2" y="-2"/>
				<Anchor point="BOTTOMRIGHT" x="-2" y="2"/>
			</Anchors>
		</PushedTexture>
		<HighlightTexture name="$parentHighlightTexture" inherits="NeuronCheckButtonTextureTemplate" alphaMode="ADD" file="Interface\Buttons\ButtonHilight-Square">
			<Anchors>
				<Anchor point="TOPLEFT" x="3" y="-3"/>
				<Anchor point="BOTTOMRIGHT" x="-3" y="3"/>
			</Anchors>
		</HighlightTexture>
		<CheckedTexture name="$parentCheckedTexture" inherits="NeuronCheckButtonTextureTemplate" alphaMode="ADD" file="Interface\Buttons\CheckButtonHilight">
			<Anchors>
				<Anchor point="TOPLEFT" x="3" y="-3"/>
				<Anchor point="BOTTOMRIGHT" x="-3" y="3"/>
			</Anchors>
		</CheckedTexture>
	</CheckButton>





